{
    "manifest_version": 3,
    "name": "SharePoint Internal Field Name Viewer",
    "version": "2.0",
    "description": "Displays internal field names on SharePoint metadata pages.",
    "permissions": [
      "activeTab"
        ],
    "content_scripts": [
      {
        "matches": ["*://*/*/_layouts/15/CaseApp/CaseType/Metadata.aspx"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
}
  